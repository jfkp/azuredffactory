{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "jfkappfactory"
		},
		"app_vm_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'app_vm_server'"
		},
		"datalakegen2jfk_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakegen2jfk_service'"
		},
		"jfk_appdb_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'jfk_appdb_service'"
		},
		"jfkworkspacesynapce_pool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'jfkworkspacesynapce_pool'"
		},
		"app_vm_server_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\jfkadmin\\Downloads\\nginx-1.25.4\\nginx-1.25.4\\logs"
		},
		"app_vm_server_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "jfkadmin"
		},
		"databrickworkspace_service_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0404-095023-2tpdzvdx"
		},
		"datalakegen2jfk_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen2jfk.dfs.core.windows.net/"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/5b23d2d2-b0a2-4b60-856a-131d7b3b4aef/resourceGroups/azure-data-rg/providers/Microsoft.Storage/storageAccounts/datalakegen2jfk"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "jfkappfactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalakegen2jfk_service_accountKey": {
						"value": "[parameters('datalakegen2jfk_service_accountKey')]"
					},
					"jfk_appdb_service_connectionString": {
						"value": "[parameters('jfk_appdb_service_connectionString')]"
					},
					"jfkworkspacesynapce_pool_connectionString": {
						"value": "[parameters('jfkworkspacesynapce_pool_connectionString')]"
					},
					"databrickworkspace_service_properties_typeProperties_existingClusterId": {
						"value": "[parameters('databrickworkspace_service_properties_typeProperties_existingClusterId')]"
					},
					"datalakegen2jfk_service_properties_typeProperties_url": {
						"value": "[parameters('datalakegen2jfk_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "jfkappfactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'jfkappfactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "jfkappfactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"app_vm_server_password": {
						"value": "[parameters('app_vm_server_password')]"
					},
					"app_vm_server_properties_typeProperties_host": {
						"value": "[parameters('app_vm_server_properties_typeProperties_host')]"
					},
					"app_vm_server_properties_typeProperties_userId": {
						"value": "[parameters('app_vm_server_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'jfkappfactory_ArmTemplate_1')]"
			]
		},
		{
			"name": "jfkappfactory_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'jfkappfactory_ArmTemplate_2')]"
			]
		}
	]
}